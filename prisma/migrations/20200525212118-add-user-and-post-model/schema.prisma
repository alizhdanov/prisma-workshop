datasource db {
  provider = "postgresql"
  url = "***"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @default(autoincrement()) @id
  email String @unique
  bio   String
  image String
}

model Post {
  id        Int      @default(autoincrement()) @id
  title     String
  body      String
  createdAt DateTime @default(now())
  updateAt  DateTime @updatedAt
}